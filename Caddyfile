coasv.suresht.com {
    # Serve static files from staticfiles directory
    handle_path /static/* {
        root /srv/coasv/staticfiles/
        file_server
    }

    # Proxy all other requests to Gunicorn
    handle {
        reverse_proxy http://localhost:8000

    }

    # Access & error logging to stdout
    #log {
    #    output stdout
    #    format console
    #}
}

